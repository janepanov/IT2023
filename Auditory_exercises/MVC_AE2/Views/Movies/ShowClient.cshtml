@model MVC_AE2.Models.Client

@{
    ViewBag.Title = "ShowClient";
}

@if (Model.Age < 18)
{
    <h1><span style="color:red">Потребен е надзор од родител или старател</span></h1>

}
else
{
    <h2>@Model.Name</h2>

    <h3>Address: @Model.Address</h3>
    <h3>Telephone: @Model.Telephone</h3>
    <h3>Age: @Model.Age</h3>
    <h3>Membership card: @Model.MembershipCard</h3>
}

